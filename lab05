from math import pi, cos, sin
v0=15
theta=pi/3
vx=v0 * cos(theta)
vy=v0 * sin(theta)

x=0
y=0
t=0
dt=0.005
g=-9.82
c1=0.5
vw=-10
 
ax=0
ay=g
f=open("VAluesX-10.txt", "w")
k=open("VAluesY-10.txt","w")

while y>=0:
   f=open("VAluesX-10.txt", "w")
   k=open("VAluesY-10.txt","w")
   y+=vy*dt
   x+=vx*dt
   vy+=ay*dt
   vx+=ax*dt
   ay=g-c1*vy
   ax=-c1 *(vx-vw)
   t+=dt
   print(y)
   f.write(str(x) + "\n")
   k.write(str(y)+ "\n")
   f.close()
   k.close()
